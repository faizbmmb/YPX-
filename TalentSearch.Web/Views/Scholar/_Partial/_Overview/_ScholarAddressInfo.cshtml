@using TalentSearch.Core.Datawarehouse
@using TalentSearch.Web.Models.Configurations

@{
	tbl_datawarehouse _Data = (tbl_datawarehouse)@ViewData["UserProfileDetail"];
}
<div class="card mb-5 mb-xl-10" id="kt_profile_details_view">
	<div class="card-header cursor-pointer">
		<div class="card-title m-0">
			<h3 class="fw-bold m-0">Address Information</h3>
		</div>
	</div>
	<div class="card-body p-9">
		
		<div>
			<div class="card-title mb-7">
				<h4 class="fw-bold m-0">Home Address</h4>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 1</label>
				<div class="col-lg-8">
					<span class="fw-semibold fs-6 text-gray-800">@_Data.tbl_Scholar.sch_homeadd_line_1</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 2</label>
				<div class="col-lg-8">
					<span class="fw-semibold fs-6 text-gray-800">@_Data.tbl_Scholar.sch_homeadd_line_2</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 3</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_line_3</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">City</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_city</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Postcode</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_postcode</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">State</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_state</span>
				</div>
			</div>
		</div>
		<div>
			<div class="card-title mb-7">
				<h4 class="fw-bold m-0">Mailing Address</h4>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 1</label>
				<div class="col-lg-8">
					<span class="fw-semibold fs-6 text-gray-800">@_Data.tbl_Scholar.sch_homeadd_line_1</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 2</label>
				<div class="col-lg-8">
					<span class="fw-semibold fs-6 text-gray-800">@_Data.tbl_Scholar.sch_homeadd_line_2</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Address Line 3</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_line_3</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">City</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_city</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">Postcode</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_postcode</span>
				</div>
			</div>
			<div class="row mb-7">
				<label class="col-lg-4 fw-semibold text-muted">State</label>
				<div class="col-lg-8 fv-row">
					<span class="fw-semibold text-gray-800 fs-6">@_Data.tbl_Scholar.sch_homeadd_state</span>
				</div>
			</div>
		</div>
		
	</div>
</div>